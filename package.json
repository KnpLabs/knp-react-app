{
  "name": "knp-react-app",
  "version": "0.1.0",
  "dependencies": {
    "ramda": "^0.26.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-functional-lifecycle": "^3.0.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.4.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "build-autoprefixed-css": "postcss build/static/css/*.css --replace --use autoprefixer --verbose --map false",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js build-autoprefixed-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.8",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "react-scripts": "^2.1.5",
    "react-test-renderer": "^16.8.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
